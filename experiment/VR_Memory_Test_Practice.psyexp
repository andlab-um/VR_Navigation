        <Param val="shuffle = util.shuffle&amp;#10;win = psychoJS.window&amp;#10;thisExp = psychoJS.experiment" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="kinships = [&quot;\u7238\u7238&quot;, &quot;\u5988\u5988&quot;, &quot;\u513f\u5b50&quot;, &quot;\u5973\u513f&quot;, &quot;\u54e5\u54e5&quot;, &quot;\u5f1f\u5f1f&quot;, &quot;\u59d0\u59d0&quot;, &quot;\u59b9\u59b9&quot;, &quot;\u7237\u7237&quot;, &quot;\u5916\u516c&quot;, &quot;\u5976\u5976&quot;, &quot;\u5916\u5a46&quot;, &quot;\u5b59\u5b50&quot;, &quot;\u5916\u5b59&quot;, &quot;\u5b59\u5973&quot;, &quot;\u5916\u5b59\u5973&quot;, &quot;\u53d4\u53d4&quot;, &quot;\u4f2f\u4f2f&quot;, &quot;\u8205\u8205&quot;, &quot;\u59d1\u59d1&quot;, &quot;\u59e8\u5988&quot;, &quot;\u4f84\u5b50&quot;, &quot;\u5916\u7525&quot;, &quot;\u4f84\u5973&quot;, &quot;\u5916\u7525\u5973&quot;, &quot;\u5802\u54e5&quot;, &quot;\u5802\u59d0&quot;, &quot;\u5802\u5f1f&quot;, &quot;\u5802\u59b9&quot;, &quot;\u8868\u54e5&quot;, &quot;\u8868\u59d0&quot;, &quot;\u8868\u5f1f&quot;, &quot;\u8868\u59b9&quot;];&amp;#10;shuffle(kinships);&amp;#10;thisExp.addData(&quot;kinships&quot;, kinships);&amp;#10;thisExp.nextEntry();&amp;#10;stimuli = [];&amp;#10;x_pos = [(- 0.9), (- 0.8), (- 0.7)];&amp;#10;for (var i = 0, _pj_a = kinships.length; (i &lt; _pj_a); i += 1) {&amp;#10;    stimuli.push(new visual.TextStim({&quot;win&quot;: win, &quot;name&quot;: i.toString(), &quot;text&quot;: kinships[i], &quot;font&quot;: &quot;Open Sans&quot;, &quot;pos&quot;: [x_pos[Number.parseInt((i / 11))], (0.5 - (0.1 * (i % 11)))], &quot;height&quot;: 0.05, &quot;color&quot;: &quot;white&quot;, &quot;colorSpace&quot;: &quot;rgb&quot;}));&amp;#10;    stimuli[i].setAutoDraw(true);&amp;#10;}&amp;#10;drag_in_process = false;&amp;#10;clicked_stim = null;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
